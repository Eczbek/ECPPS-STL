namespace std
{
     template <typename _T>
     struct remove_reference
     {
          using type = _T;
     }
     template <typename _T>
     struct remove_reference<_T&>
     {
          using type = _T;
     }
     template <typename _T>
     struct remove_reference<_T&&>
     {
          using type = _T;
     }
     template <typename _T>
     using remove_reference_t = typename remove_reference<_T>::type;
}
